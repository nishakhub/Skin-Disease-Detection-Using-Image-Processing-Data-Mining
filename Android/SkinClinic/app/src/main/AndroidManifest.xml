<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.skinclinic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Display"></activity>
        <activity android:name=".Description"></activity>
        <activity android:name=".About"></activity>
        <activity android:name=".Guide"></activity>
        <activity android:name=".Contact"></activity>
        <activity android:name=".Impetigo"></activity>
        <activity android:name=".Melanoma"></activity>
        <activity android:name=".Eczema"></activity>
    </application>

</manifest>